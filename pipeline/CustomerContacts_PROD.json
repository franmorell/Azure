				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "DECLARE @LastLoadDate DATETIME\nSELECT @LastLoadDate = MAX(EndDateTime)\nFROM load.Run\nWHERE SourceName = '@{pipeline().parameters.SourceName}'\nAND [Status] = 'Success';\n\nSELECT\n    ds.SYSTEM_NAME as ERP,\n    hc.CUSTOMER_NUMBER AS CUSTOMER_ID,\n    scc.NAME,\n    scc.BUSINESS_UNIT,\n    hcc.SYSTEM_ID AS COMPANY_CODE,\n    scc.SET_ID,\n    scc.PRIMARY_CONTACT_FLAG,\n    hcc.FIRST_NAME AS CONTACT_FIRST_NAME,\n    hcc.LAST_NAME AS CONTACT_LAST_NAME,\n    scc.ROLE,\n    REPLACE(REPLACE(scc.PHONE_NUMBER, CHAR(13),''), CHAR(10), '') AS PHONE_NUMBER,\n    REPLACE(REPLACE(scc.EMAIL_ADDRESS, CHAR(13),''), CHAR(10), '') AS EMAIL_ADDRESS,\n    scc.FAX\nFROM\n    dv.H_CUSTOMER_CONTACT hcc WITH(NOLOCK)\n    LEFT JOIN dv.DICT_SYSTEM ds WITH(NOLOCK) ON hcc.SYSTEM_ID = ds.SYSTEM_ID\n    JOIN dv.tfnS_CUSTOMER_CONTACT_BY_DATE(GETDATE()) scc ON hcc.CUSTOMER_CONTACT_HASH_KEY = scc.CUSTOMER_CONTACT_HASH_KEY\n    JOIN dv.tfnL_CUSTOMER_CUSTOMER_CONTACT_BY_DATE(GETDATE()) lccc ON hcc.CUSTOMER_CONTACT_HASH_KEY = lccc.CUSTOMER_CONTACT_HASH_KEY\n    JOIN dv.H_CUSTOMER hc WITH(NOLOCK) ON lccc.CUSTOMER_HASH_KEY = hc.CUSTOMER_HASH_KEY\n    JOIN dv.tfnS_CUSTOMER_CONTACT_DELETION_BY_DATE(GETDATE()) sccd ON hcc.CUSTOMER_CONTACT_HASH_KEY = sccd.CUSTOMER_CONTACT_HASH_KEY\nWHERE 1=1\n    AND sccd.IS_DELETED = 'NO'\n    AND hcc.SYSTEM_ID IN ('AM-US01','AM-CA01','EU-DE01')\n    AND (hcc.LOAD_DTS > @LastLoadDate \n        OR scc.LOAD_DTS > @LastLoadDate \n        OR lccc.LOAD_DTS > @LastLoadDate \n        OR hc.LOAD_DTS > @LastLoadDate\n    )",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
