						"sqlReaderQuery": {
							"value": "WITH DetailedCustLedgerEntryCTE AS (\n\tSELECT\n\t\t[Cust_ Ledger Entry No_],\n\t\tSUM(Amount) AS Open_Amount,\n\t\tSUM([Amount (LCY)]) AS Open_Amount_LCY\n\tFROM\n\t\tdbo.[@{pipeline().parameters.sourceSchema}$Detailed Cust_ Ledg_ Entry]\n\tGROUP BY\n\t\t[Cust_ Ledger Entry No_]\n)\nSELECT \n\t'Navision' as ERP,\n\tcle.[Customer No_] as Customer_No,\n\tCASE cle.[Document Type]\n\t\twhen 0 THEN 'N/A'\n\t\tWHEN 1 THEN 'PAYMENT'\n\t\tWHEN 2 THEN 'INVOICE'\n\t\tWHEN 3 THEN 'CREDIT MEMO'\n\t\tWHEN 4 THEN 'FINANCE CHARGE MEMO'\n\t\tWHEN 5 THEN 'REMINDER'\n\t\tWHEN 6 THEN 'REFUND'\n\t\tELSE 'UNKNOWN'\n\tEND [Document_Type],\n\tIIF(SUM(dcle.Open_Amount) >= 0, 'CREDIT', 'DEBIT') as DEBIT_CREDIT_INDICATOR,\n\tSUM(dcle.Open_Amount_LCY) as TOTAL_LOCAL_AMOUNT,\n\tSUM(dcle.Open_Amount) as TOTAL_DOC_AMOUNT,\n\tSUM(cle.[Open]) as TOTAL_OPEN_ITEMS\nFROM\n\tdbo.[@{pipeline().parameters.sourceSchema}$Cust_ Ledger Entry] cle\n\tLEFT JOIN DetailedCustLedgerEntryCTE dcle ON cle.[Entry No_] = dcle.[Cust_ Ledger Entry No_]\nGROUP BY\n\tcle.[Customer No_],\n\tCASE cle.[Document Type]\n\t\twhen 0 THEN 'N/A'\n\t\tWHEN 1 THEN 'PAYMENT'\n\t\tWHEN 2 THEN 'INVOICE'\n\t\tWHEN 3 THEN 'CREDIT MEMO'\n\t\tWHEN 4 THEN 'FINANCE CHARGE MEMO'\n\t\tWHEN 5 THEN 'REMINDER'\n\t\tWHEN 6 THEN 'REFUND'\n\t\tELSE 'UNKNOWN'\n\tEND",
							"type": "Expression"
						},
