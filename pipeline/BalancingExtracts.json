				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "DECLARE @LastLoadDate DATETIME;\nSELECT @LastLoadDate = MAX(EndDateTime)\nFROM load.Run\nWHERE SourceName = '@{pipeline().parameters.SourceName}'\nAND [Status] = 'Success';\n\nSELECT\n    ds.SYSTEM_NAME AS ERP,\n    hc.CUSTOMER_NUMBER AS CUSTOMER_ID,\n    hc.SYSTEM_ID AS COMPANY_CODE,\n    hdt.DOCUMENT_TYPE AS DOCTYPE,\n    scdtb.TOTAL_LOCAL_AMOUNT,\n    scdtb.TOTAL_DOC_AMOUNT,\n    scdtb.TOTAL_OPEN_ITEMS\nFROM\n    dv.L_CUSTOMER_DOCUMENT_TYPE_BALANCE lcdtb WITH(NOLOCK)\n    JOIN dv.H_CUSTOMER hc WITH(NOLOCK) ON lcdtb.CUSTOMER_HASH_KEY = hc.CUSTOMER_HASH_KEY\n    JOIN dv.H_DOCUMENT_TYPE hdt WITH(NOLOCK) ON lcdtb.DOCUMENT_TYPE_HASH_KEY = hdt.DOCUMENT_TYPE_HASH_KEY\n    JOIN dv.tfnS_CUSTOMER_DOCUMENT_TYPE_BALANCE_BY_DATE(GETDATE()) scdtb ON lcdtb.CUSTOMER_DOCUMENT_TYPE_HASH_KEY = scdtb.CUSTOMER_DOCUMENT_TYPE_HASH_KEY\n    LEFT JOIN dv.DICT_SYSTEM ds WITH(NOLOCK) ON hc.SYSTEM_ID = ds.SYSTEM_ID\nWHERE\n    hc.SYSTEM_ID IN (\n        'EU-PL01','EU-CZ01','EU-UK01',\n        'EU-ES01','EU-NL01','EU-NL02',\n        'EU-BE01','AP-AU01')\n    /*AND (lcdtb.LOAD_DTS > @LastLoadDate\n        OR hc.LOAD_DTS > @LastLoadDate\n        OR hdt.LOAD_DTS > @LastLoadDate\n        OR scdtb.LOAD_DTS > @LastLoadDate\n    )*/",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "SftpWriteSettings",
							"operationTimeout": "01:00:00",
							"useTempFileRename": true
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "ERP",
									"type": "String"
								},
								"sink": {
									"name": "ERP",
									"type": "String"
								}
