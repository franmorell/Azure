						"query": "select \t\n\t'AS400' as ERP,\n\t'EU-FR01' as Company_ID,\n\tsubstr(hcpcpt, 6, 5) as Customer_ID,\n\t(select clnom from comafiTB.focli where clcs41 = 41 and clclie = substr(hcpcpt, 6, 5)) as Customer_name,\n\n\thcpmff as total_Amount,\n\thcpmff as Open_amt,\n\tconcat(concat(concat(hcpcpt,(concat(concat(concat(concat (hcpss*100+hcpaa, '-'), hcpmm), '-'), hcpjj))),'-'),hcpran) as ApplicationID,\n\ttrim(hcpnfa) as document_no,\n\tCase \n\t\tWhen hcpfar = 'F' then 'INVOICE'\n\t\tWhen hcpfar = 'A' then 'CREDIT MEMO'\n\t\tWhen hcpfar = 'R' then 'PAYMENT'\n\t\tWhen hcpfar = 'D' then\n\t\t\tcase \n\t\t\tWhen hcpmrg = 'IMP' then 'DISHONORED'\n\t\t\tElse  'REFUND'\n\t\t\tEnd \n\t\telse\n\t\t\tcase \n\t\t\twhen hcpmff > 0 then 'FINANCE CHARGE MEMO'\n\t\t\telse ''\n\t\t\tend \n\tend as document_type,\n\tconcat(concat(concat(concat(concat (hcpss*100+hcpaa, '-'), substr(100+hcpmm, 2, 2)), '-'), substr(100+hcpjj, 2, 2)),' 00:00:00') as Posting_Date, \n\thcpmrg as terms_desc,\n\tCase \n        When hcpecm<>0 then \n        concat(concat(concat(concat(concat (hcpecs*100+hcpeca, '-'), substr(100+hcpecm, 2, 2)), '-'), substr(100+hcpecj, 2, 2)),' 00:00:00') \n        else \n        concat(concat(concat(concat(concat (hcpss*100+hcpaa, '-'), substr(100+hcpmm, 2, 2)), '-'), substr(100+hcpjj, 2, 2)),' 00:00:00') \n        end  \n\tas Net_Due_Date,\n\thcpcdv as currency,\n\t'EUR' as company_code_currency,\n\tCAST(NULL AS VARCHAR(20)) as clearing_date,\n\t'DY.CO' as Business_Unit,\n\t'FR' as Ship_to_Country_Code,\n\t'1' as open\nfrom comptaTB.fdhclie\nwhere hcplet = ' '\nand hcpfar <> 'R'\nUNION ALL\nselect \t\n\t'AS400' as ERP,\n\t'EU-FR01' as Company_ID,\n\tsubstr(hcpcpt, 6, 5) as Customer_ID,\n\t(select clnom from comafiTB.focli where clcs41 = 41 and clclie = substr(hcpcpt, 6, 5)) as Customer_name,\n\thcpmff as total_Amount,\n\t0 as Open_amt,\n\tconcat(concat(concat(hcpcpt,(concat(concat(concat(concat (hcpss*100+hcpaa, '-'), hcpmm), '-'), hcpjj))),'-'),hcpran) as ApplicationID,\n\ttrim(hcpnfa) as document_no,\n\tCase \n\t\tWhen hcpfar = 'F' then 'INVOICE'\n\t\tWhen hcpfar = 'A' then 'CREDIT MEMO'\n\t\tWhen hcpfar = 'R' then 'PAYMENT'\n\t\tWhen hcpfar = 'D' then\n\t\t\tcase \n\t\t\tWhen hcpmrg = 'IMP' then 'DISHONORED'\n\t\t\tElse  'REFUND'\n\t\t\tEnd \n\t\telse\n\t\t\tcase \n\t\t\twhen hcpmff > 0 then 'FINANCE CHARGE MEMO'\n\t\t\telse ''\n\t\t\tend \n\tend as document_type,\n\tconcat(concat(concat(concat(concat (hcpss*100+hcpaa, '-'), (hcpmm)), '-'), (hcpjj)),' 00:00:00') as Posting_Date,\n\thcpmrg as terms_desc,\n\tCase \n        When hcpecm<>0 then \n        concat(concat(concat(concat(concat (hcpecs*100+hcpeca, '-'), substr(100+hcpecm, 2, 2)), '-'), substr(100+hcpecj, 2, 2)),' 00:00:00') \n        else \n        concat(concat(concat(concat(concat (hcpss*100+hcpaa, '-'), substr(100+hcpmm, 2, 2)), '-'), substr(100+hcpjj, 2, 2)),' 00:00:00') \n        end  \n\tas Net_Due_Date,\n\thcpcdv as currency,\n\t'EUR' as company_code_currency,\n\tconcat(concat(concat(concat(concat(substr(concat(hcpdls,hcpdal), 1, 4), '-'), substr(hcpdal, 3, 2)), '-'), substr(hcpdal, 5, 2)),' 00:00:00')  as clearing_date,\n\t'DY.CO' as Business_Unit,\n\t'FR' as Ship_to_Country_Code,\n\t'0' as Open\t\nfrom comptaTB.fdhclie\nwhere hcplet <> ' '\nand hcpfar <> 'R'\t\n"
					},
