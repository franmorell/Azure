				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "DECLARE @LastLoadDate DATETIME\nSELECT @LastLoadDate = MAX(EndDateTime)\nFROM load.Run\nWHERE SourceName = '@{pipeline().parameters.SourceName}'\nAND [Status] = 'Success';\n\nSELECT\n    ds.SYSTEM_NAME AS ERP,\n    hp.SYSTEM_ID AS COMPANY_CODE,\n    hp.PAYMENT_NUMBER AS APPLICATION_ID,\n    sp.CREATION_DATE,\n    hc.CUSTOMER_NUMBER AS CUSTOMER_ID,\n    scm.CUSTOMER_NAME,\n    sp.BUSINESS_UNIT,\n    sp.REGION,\n    sp.TRANSACTION_ID,\n    hdt.DOCUMENT_TYPE,\n    sp.TRANSACTION_DATE,\n    sp.TRANSACTION_CURRENCY_CODE,\n    sp.REPORTING_TX_AMOUNT,\n    sp.TX_AMOUNT_APPLIED,\n    sp.INVOICE_NUMBER,\n    ISNULL(sp.DOCUMENT_DATE, sp.CREATION_DATE) AS DOCUMENT_DATE,\n    sp.ORIGINAL_TX_AMOUNT,\n    sp.PAYMENT_METHOD,\n    hcc.COUNTRY_CODE,\n    poda.ORIGINAL_DOCUMENT_APPLICATION_ID\nFROM\n    dv.H_PAYMENT hp WITH(NOLOCK)\n    LEFT JOIN dv.DICT_SYSTEM ds WITH(NOLOCK) ON hp.SYSTEM_ID = ds.SYSTEM_ID\n    JOIN dv.tfnS_PAYMENT_BY_DATE(GETDATE()) sp ON hp.PAYMENT_HASH_KEY = sp.PAYMENT_HASH_KEY\n    JOIN dv.tfnL_PAYMENT_CUSTOMER_BY_DATE(GETDATE()) lpc ON hp.PAYMENT_HASH_KEY = lpc.PAYMENT_HASH_KEY\n    JOIN dv.H_CUSTOMER hc WITH(NOLOCK) ON lpc.CUSTOMER_HASH_KEY = hc.CUSTOMER_HASH_KEY\n    LEFT JOIN dv.tfnS_CUSTOMER_MASTER_BY_DATE(GETDATE()) scm ON hc.CUSTOMER_HASH_KEY = scm.CUSTOMER_HASH_KEY\n    JOIN dv.tfnL_PAYMENT_COUNTRY_BY_DATE(GETDATE()) lpcc ON hp.PAYMENT_HASH_KEY = lpcc.PAYMENT_HASH_KEY\n    JOIN dv.H_COUNTRY hcc WITH(NOLOCK) ON lpcc.COUNTRY_HASH_KEY = hcc.COUNTRY_HASH_KEY\n    JOIN dv.tfnL_PAYMENT_DOCUMENT_TYPE_BY_DATE(GETDATE()) lpdt ON hp.PAYMENT_HASH_KEY = lpdt.PAYMENT_HASH_KEY\n    JOIN dv.H_DOCUMENT_TYPE hdt WITH(NOLOCK) ON lpdt.DOCUMENT_TYPE_HASH_KEY = hdt.DOCUMENT_TYPE_HASH_KEY\n    JOIN dv.tfnS_PAYMENT_DELETION_BY_DATE(GETDATE()) spd ON hp.PAYMENT_HASH_KEY = spd.PAYMENT_HASH_KEY\n    LEFT JOIN dv.tfnPAYMENT_ORIGINAL_DOCUMENT_APPLICATION_ID_BY_DATE(GETDATE()) poda ON hp.PAYMENT_HASH_KEY = poda.PAYMENT_HASH_KEY\nWHERE 1=1\n    AND hp.SYSTEM_ID IN (\n        'EU-PL01','EU-CZ01','EU-UK01',\n        'EU-ES01','EU-NL01','EU-NL02',\n        'EU-BE01','AP-AU01')\n    AND spd.IS_DELETED = 'NO'\n    AND sp.CREATION_DATE >= DATEADD(MONTH,-6,GETDATE())\n    AND sp.OPEN_INDICATOR = 0\n\t/*AND (\n\t\thp.LOAD_DTS > @LastLoadDate\n\t\tOR sp.LOAD_DTS > @LastLoadDate \n\t\tOR lpc.LOAD_DTS > @LastLoadDate\n\t\tOR hc.LOAD_DTS > @LastLoadDate\n\t\t--OR scm.LOAD_DTS > @LastLoadDate\n\t\tOR lpcc.LOAD_DTS > @LastLoadDate\n\t\tOR hcc.LOAD_DTS > @LastLoadDate\n\t\tOR lpdt.LOAD_DTS > @LastLoadDate\n\t\tOR hdt.LOAD_DTS > @LastLoadDate\n\t\tOR poda.LOAD_DTS > @LastLoadDate\n\t)*/",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
