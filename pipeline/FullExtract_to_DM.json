					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "\nDECLARE @LastLoadDate DATETIME\nSELECT @LastLoadDate = MAX(EndDateTime)\nFROM load.Run\nWHERE SourceName = '@{pipeline().parameters.SourceName}';\n\nSELECT \n    ds.SYSTEM_NAME AS ERP,\n    hcld.SYSTEM_ID AS COMPANY_CODE,\n    hc.CUSTOMER_NUMBER AS CUSTOMER_ID,\n    scm.CUSTOMER_NAME,\n    scld.DOCUMENT_AMOUNT,\n    scld.OPEN_AMOUNT,\n    hcld.CUSTOMER_LEDGER_ENTRY_ID AS APPLICATION_ID,\n    scld.DOCUMENT_NUMBER,\n    hdt.DOCUMENT_TYPE,\n    scld.POSTING_DATE,\n    hpt.PAYMENT_TERMS_CODE AS TERMS_DESC,\n    scld.DUE_DATE,\n    scld.DOCUMENT_CURRENCY_CODE,\n    scld.ENTITY_CURRENCY_CODE AS COMPANY_CURRENCY_CODE,\n    scld.PURCHASE_ORDER,\n    scld.ORDER_NUMBER,\n    scld.DISCOUNT_AMOUNT,\n    scld.DISCOUNT_DUE_DATE,\n    IIF(scld.CLEARING_DATE IS NULL OR scld.CLEARING_DATE < scld.PURCHASE_ORDER, scld.DUE_DATE, scld.CLEARING_DATE) AS CLEARING_DATE,\n    scld.BUSINESS_UNIT,\n    scld.RETENTION_INDICATOR,\n    scld.RETENTION_AMOUNT,\n    hcc.COUNTRY_CODE AS SHIP_TO_COUNTRY_CODE,\n    ssp.SALES_PERSON_NAME AS SALES_PERSON,\n    scld.KOPRO_CODE,\n    scld.DESCRIPTION,\n    scld.RETENTION_DUE_DATE,\n    scld.USER_ID\nFROM\n    dv.H_CUSTOMER_LEDGER_DOCUMENT hcld WITH(NOLOCK)\n    LEFT JOIN dv.DICT_SYSTEM ds WITH(NOLOCK) ON hcld.SYSTEM_ID = ds.SYSTEM_ID\n    JOIN dv.tfnL_CUSTOMER_LEDGER_DOCUMENT_CUSTOMER_BY_DATE(GETDATE()) lcldc ON hcld.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY = lcldc.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY\n    JOIN dv.H_CUSTOMER hc WITH(NOLOCK) ON lcldc.CUSTOMER_HASH_KEY = hc.CUSTOMER_HASH_KEY\n    LEFT JOIN dv.tfnS_CUSTOMER_MASTER_BY_DATE(GETDATE()) scm ON hc.CUSTOMER_HASH_KEY = scm.CUSTOMER_HASH_KEY\n    JOIN dv.tfnS_CUSTOMER_LEDGER_DOCUMENT_BY_DATE(GETDATE()) scld ON hcld.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY = scld.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY\n    JOIN dv.tfnL_CUSTOMER_LEDGER_DOCUMENT_DOCUMENT_TYPE_BY_DATE(GETDATE()) lclddt ON hcld.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY = lclddt.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY\n    JOIN dv.H_DOCUMENT_TYPE hdt WITH(NOLOCK) ON lclddt.DOCUMENT_TYPE_HASH_KEY = hdt.DOCUMENT_TYPE_HASH_KEY\n    JOIN dv.tfnL_CUSTOMER_LEDGER_DOCUMENT_PAYMENT_TERMS_BY_DATE(GETDATE()) lcldpt ON hcld.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY = lcldpt.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY\n    JOIN dv.H_PAYMENT_TERMS hpt WITH(NOLOCK) ON lcldpt.PAYMENT_TERMS_HASH_KEY = hpt.PAYMENT_TERMS_HASH_KEY\n    JOIN dv.tfnL_CUSTOMER_LEDGER_DOCUMENT_SHIP_TO_COUNTRY_BY_DATE(GETDATE()) lcldstc ON hcld.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY = lcldstc.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY\n    JOIN dv.H_COUNTRY hcc WITH(NOLOCK) ON lcldstc.COUNTRY_HASH_KEY = hcc.COUNTRY_HASH_KEY\n    JOIN dv.tfnL_CUSTOMER_LEDGER_DOCUMENT_SALES_PERSON_BY_DATE(GETDATE()) lcldsp ON hcld.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY = lcldsp.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY\n    LEFT JOIN dv.tfnS_SALES_PERSON_BY_DATE(GETDATE()) ssp ON lcldsp.SALES_PERSON_HASH_KEY = ssp.SALES_PERSON_HASH_KEY\n    JOIN dv.tfnS_CUSTOMER_LEDGER_DOCUMENT_DELETION_BY_DATE(GETDATE()) scldd ON hcld.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY = scldd.CUSTOMER_LEDGER_DOCUMENT_HASH_KEY\nWHERE 1=1\n    --AND hcld.SYSTEM_ID = 'EU-DE01'\n    AND scldd.IS_DELETED = 'NO'\n    AND scld.OPEN_INDICATOR = 0\n    AND scld.POSTING_DATE >= DATEADD(YEAR,-2,GETDATE())\n\t-- AND hcld.LOAD_DTS > @LastLoadDate \n\t-- AND lcldc.LOAD_DTS > @LastLoadDate \n\t-- AND scm.LOAD_DTS > @LastLoadDate \n\t-- AND scld.LOAD_DTS > @LastLoadDate \n\t-- AND lclddt.LOAD_DTS > @LastLoadDate\n\t-- AND hdt.LOAD_DTS > @LastLoadDate \n\t-- AND lcldpt.LOAD_DTS > @LastLoadDate \n\t-- AND hpt.LOAD_DTS > @LastLoadDate \n\t-- AND lcldstc.LOAD_DTS > @LastLoadDate \n\t-- AND hcc.LOAD_DTS > @LastLoadDate \n\t-- AND lcldsp.LOAD_DTS > @LastLoadDate \n\t-- AND ssp.LOAD_DTS > @LastLoadDate ",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
